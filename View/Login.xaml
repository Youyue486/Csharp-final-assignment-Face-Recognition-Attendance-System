<UserControl x:Class="Csharp_final_assignment_Face_Recognition_Attendance_System.View.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Csharp_final_assignment_Face_Recognition_Attendance_System.View"
        xmlns:vm="clr-namespace:Csharp_final_assignment_Face_Recognition_Attendance_System.ViewModel"
        d:DataContext="{d:DesignInstance Type=vm:LoginViewModel}"
        d:DesignHeight="450"
        d:DesignWidth="800"
        d:Background="White"
        mc:Ignorable="d">
    <Grid>
        <Button Content="进入管理员界面" HorizontalAlignment="Center" VerticalAlignment="Center" Height="50" Width="130"
                Command="{Binding LoginCommand}"/>
    </Grid>
</UserControl>
